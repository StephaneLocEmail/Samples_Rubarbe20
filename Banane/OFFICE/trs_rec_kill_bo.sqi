/* RECEIVE UPC */

@WIZSET(F1918=0);
@WIZSET(F1919=0);
@WIZSET(F1920=0);

UPDATE REC_REG SET F1918=F1918-@WIZGET(F1918),F1919=F1919-@WIZGET(F1919),F1920=F1920-@WIZGET(F1920) 
WHERE F1032=@WIZGET(F1032) AND F1101=@WIZGET(F1101);

@FMT(CMP,@WIZGET(F1918)=0,,®WIZRPL(CASE=@WIZGET(F1918)));
@FMT(CMP,@WIZGET(F1919)=0,,®WIZRPL(UNIT=@WIZGET(F1919)));
@FMT(CMP,@WIZGET(F1920)=0,,®WIZRPL(WEIGHT=@WIZGET(F1920)));

@WIZRPL(ENTRY=@WIZGET(F01));
@EXEC(FCT=3510);

